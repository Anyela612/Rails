<h2 class= "text-center">Formulario de contacto</h2>
<div class= "container">
<br><br><br><br>
<form>
  <div class="mb-3">
  
  <div class="mb-3">
    <label for="nombre" class="form-label">Nombre</label>
    <input type="nombre" class="form-control" id="nombre">
</div>

<div class="mb-3">
    <label for="numero" class="form-label">Numero de contacto</label>
    <input type="telefono" class="form-control" id="numeroContacto">
</div>

    <label for="exampleInputEmail1" class="form-label">Correo electronico</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">
    </div>

  <div class="mb-3">
    <label for="observacion" class="form-label">Observaciones</label>
    <input type="password" class="form-control" id="observacion">
</div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>